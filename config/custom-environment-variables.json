{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "logger": {
    "level": "LOG_LEVEL"
  },
  "server": {
    "port": "SERVER_PORT"
  },
  "core": {
    "sizes": {
      "small": "CORE_IDS_RANGES_SIZES_SMALL",
      "medium": "CORE_IDS_RANGES_SIZES_MEDIUM",
      "large": "CORE_IDS_RANGES_SIZES_LARGE"
    }
  },
  "db": {
    "type": "DB_TYPE",
    "host": "DB_HOST",
    "port": {
      "__name": "DB_PORT",
      "__format": "number"
    },
    "username": "DB_USERNAME",
    "password": "DB_PASSWORD",
    "database": "DB_NAME"
  }
}
